<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="SingleLineRadioGroup">

        <attr name="rg_sl_FontFamily" format="reference" />
        <attr name="rg_sl_TextSize" format="dimension" />
        <attr name="rg_sl_Orientation" format="integer" />
        <attr name="rg_sl_Background" format="reference" />
        <attr name="rg_sl_TextColor" format="color" />
        <attr name="rg_sl_TextSelectorColor" format="color" />
        <attr name="rg_sl_PaddingStart" format="dimension" />
        <attr name="rg_sl_PaddingTop" format="dimension" />
        <attr name="rg_sl_PaddingEnd" format="dimension" />
        <attr name="rg_sl_PaddingBottom" format="dimension" />
        <attr name="rg_sl_Listitem" format="string" />

    </declare-styleable>


    <declare-styleable name="MultiLineRadioGroup">
        <attr name="rg_ml_SpanCount" format="integer" />
        <attr name="rg_ml_Spacing" format="integer" />
        <attr name="rg_ml_Background" format="reference" />
        <attr name="rg_ml_TextSelector" format="color" />
        <attr name="rg_ml_IncludeEdge" format="boolean" />
        <attr name="rg_ml_Listitem" format="reference" />
    </declare-styleable>

    <declare-styleable name="RowItemRadioGroup">
        <attr name="rg_ri_TextSize" format="dimension" />
        <attr name="rg_ri_Background" format="reference" />
        <attr name="rg_ri_TextSelector" format="color" />
        <attr name="rg_ri_Textcolor" format="color" />
        <attr name="rg_ri_Orientation" format="integer" />
    </declare-styleable>
</resources>